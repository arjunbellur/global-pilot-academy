{
  "name": "global-pilot-academy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "cf:build": "npx @cloudflare/next-on-pages@1",
    "cf:deploy": "npm run cf:build && npx wrangler pages deploy .vercel/output/static",
    "cf:dev": "npx wrangler pages dev .vercel/output/static",
    "cf:preview": "npm run cf:build && npm run cf:dev",
    "pages:build": "npx @cloudflare/next-on-pages@1 build"
  },
  "dependencies": {
    "lucide-react": "^0.294.0",
    "next": "13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.0",
    "wrangler": "^4.33.2"
  }
}
